    "checkout": "/checkout",
    "webhook": "/api/webhooks/stripe"
  },
  "policy_urls": {
    "privacy": "/legal/confidentialite",
    "terms": "/legal/cgv",
    "refunds": "/legal/retours",
    "shipping": "/legal/livraison"
  },
  "scaffold_depth": "full",
  "generated_files": [],
  "answers": {},
  "deployment": {
    "platform": "vercel",
    "region": "iad1",
    "mcp": {
      "enabled": false,
      "max_duration": 60
    }
  }
